<server description="MicroProfile Metrics Demo">
    <featureManager>
        <feature>microProfile-4.0</feature>
        <feature>cdi-2.0</feature>
    </featureManager>

    <quickStartSecurity userName="admin" userPassword="admin"/>
    <httpEndpoint httpPort="${default.http.port}" httpsPort="${default.https.port}" id="defaultHttpEndpoint" host="*"/>
    <webApplication location="../microprofile-metrics-demo.war" contextRoot="${app.context.root}"/>
</server>
